[req]
distinguished_name = req_distinguished_name
req_extensions = v3_req
prompt = no

[req_distinguished_name]
CN = localhost

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
# You can add more DNS names or IP addresses if needed
# DNS.2 = mybroker.example.com
# IP.1 = 127.0.0.1